            <font>
                <Font name="Bookshelf Symbol 7" size="34.0" />
            </font>
        </Label>
        <ImageView fitHeight="45.0" fitWidth="45.0" layoutX="1133.0" layoutY="555.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@home.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="45.0" fitWidth="45.0" layoutX="1133.0" layoutY="626.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@profile.png" />
            </image>
        </ImageView>
