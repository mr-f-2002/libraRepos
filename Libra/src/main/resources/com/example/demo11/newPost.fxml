                        <Image url="@libraLogo.png" />
                    </image>
                </ImageView>
            </children></AnchorPane>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="15.0" layoutY="9.0" prefHeight="181.0" prefWidth="206.0" text="Libra" textFill="WHITE">
